{
  "name": "@whatskit/wechatwork-action",
  "description": "@whatskit/wechatwork-action",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Billgo <hi@billgo.me>",
  "bugs": "https://github.com/whatskit/node-packages/issues",
  "repository": "git@github.com:whatskit/node-packages",
  "prettier": "prettier-config-whatskit",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "yarn run build",
    "clean": "rimraf coverage dist build tmp",
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts-resolve && ncc build -o lib --source-map --license licenses.txt",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/jest": "~29.5.2",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "~5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vercel/ncc": "^0.36.1",
    "@whatskit/cross-env": "^0.1.0",
    "@whatskit/kill-port": "^0.1.0",
    "@whatskit/rimraf": "^0.1.1",
    "eslint": "~8.42.0",
    "eslint-config-prettier": "~8.8.0",
    "eslint-plugin-jest": "~27.2.1",
    "jest": "~29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "~2.8.8",
    "prettier-config-whatskit": "^0.4.0",
    "ts-jest": "~29.1.0",
    "tsup": "6.7.0",
    "tsutils": "~3.21.0",
    "typescript": "~5.1.3"
  },
  "eslintIgnore": [
    "src/**/*.test.tsx",
    "lib/*",
    "docs/*",
    "dist/*",
    "build/*",
    "coverage/*",
    "fixtures/*",
    "custom/*",
    "pages/*",
    "umd/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">= 16.13"
  }
}
